{
  "manifest_version": 2,

  "name": "Tab Butler",
  "description": "",
  "version": "0.0.1",
  "icons": {
    "16": "icon-16.png",
    "128": "icon-128.png"
  },
  "options_page": "index.html",
  "background": {
    "page": "index.html"
  },
  "browser_action": {
    "default_icon": "icon-16.png",
    "default_popup": "index.html#/popup"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "<all_urls>",
    "tabs",
    "bookmarks",
    "background",
    "alarms",
    "storage"
  ]
}
